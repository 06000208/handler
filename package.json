{
  "name": "@a06000208/handler",
  "version": "0.0.2",
  "description": "Extendable javascript handler for objects, events, and modules",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/06000208/handler.git"
  },
  "author": "06000208",
  "license": "Unlicense",
  "bugs": {
    "url": "https://github.com/06000208/handler/issues"
  },
  "homepage": "https://github.com/06000208/handler#readme",
  "scripts": {
    "test": "echo \"Error: no tests\" && exit 1",
    "lint": "eslint . --ext .js,.mjs,.cjs",
    "fix": "eslint . --ext .js,.mjs,.cjs --fix",
    "dev": "node esbuild.config.js",
    "ts": "npx tsc",
    "build": "node esbuild.config.js --production && npx tsc",
    "docs": "echo \"Error: no docs setup\" && exit 1",
    "prepare": "npm run build"
  },
  "files": [
    "dist",
    "src"
  ],
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./src/index.js",
      "require": "./dist/index.cjs",
      "default": "./dist/index.browser.js"
    },
    "./browser": {
      "types": "./dist/index.browser.d.ts",
      "import": "./dist/index.browser.js",
      "default": "./dist/index.browser.js"
    },
    "./cjs": {
      "types": "./dist/index.cjs.d.ts",
      "import": "./dist/index.cjs",
      "require": "./dist/index.cjs",
      "default": "./dist/index.cjs"
    }
  },
  "main": "./dist/index.cjs",
  "browser": "./dist/index.browser.min.js",
  "unpkg": "./dist/index.browser.min.js",
  "types": "./dist/index.d.ts",
  "sideEffects": false,
  "engines": {
    "node": ">=16.9.0"
  },
  "devDependencies": {
    "esbuild": "^0.17.5",
    "eslint": "^8.33.0",
    "typescript": "^4.9.5"
  }
}